{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "front",
      "image": "belalelhossany/multi-front",
      "hostname": "client",
      "essential": false,
      "memory": 128
    },
    {
      "name": "nginx",
      "image": "belalelhossany/multi-nginx",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": ["front"],
      "memory": 128
    }
  ]
}
